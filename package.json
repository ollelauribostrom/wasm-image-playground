{
  "name": "wasm-image-playground",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-waterfall": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.5.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.0.4",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production --env /wasm-image-playground/",
    "test": "jest",
    "test:all": "yarn test && yarn lint && yarn flow",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./test/coverage/lcov.info | coveralls",
    "lint": "eslint src test",
    "flow": "flow"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/test/test-setup.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/src/types/**/*.js"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/test/test-mock.js"
    }
  }
}
